<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TodayFragment">

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="@+id/tvDescr"
            app:layout_constraintStart_toEndOf="@+id/tvDescr">

            <ImageView
                android:id="@+id/tempIcon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="117dp"
                android:layout_marginEnd="53dp"
                android:layout_marginBottom="21dp"
                app:layout_constraintBottom_toTopOf="@+id/tvDescr"
                app:layout_constraintEnd_toStartOf="@+id/tempHigh"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/img" />

            <TextView
                android:id="@+id/tempHigh"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="133dp"
                android:layout_marginEnd="59dp"
                android:layout_marginBottom="10dp"
                android:text="TextView"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/tempLow"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tempIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tempLow"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginBottom="393dp"
                android:text="TextView"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tempIcon"
                app:layout_constraintTop_toBottomOf="@+id/tempHigh" />

            <TextView
                android:id="@+id/tvDescr"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="28dp"
                android:layout_marginEnd="22dp"
                android:layout_marginBottom="394dp"
                android:text="TextView"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tempLow"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tempIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>